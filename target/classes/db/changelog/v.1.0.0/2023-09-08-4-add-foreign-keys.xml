<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">

    <changeSet id="2023-09-08-3-create-table-user-role-junction" author="SursinDV">
        <addForeignKeyConstraint baseTableName="user_role_junction"
                                 baseColumnNames="user_id"
                                 referencedTableName="users"
                                 referencedColumnNames="user_id"
                                 constraintName="fk_user_role_junction_user"/>
        <addForeignKeyConstraint baseTableName="user_role_junction"
                                 baseColumnNames="role_id"
                                 referencedTableName="roles"
                                 referencedColumnNames="role_id"
                                 constraintName="fk_user_role_junction_role"/>
    </changeSet>

</databaseChangeLog>
